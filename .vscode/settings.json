{
    "chat.mcp.serverSampling": {
        "odsbox_playground/.vscode/mcp.json: odsbox-jaquel": {
            "allowedModels": [
                "copilot/gpt-5-mini",
                "copilot/auto",
                "copilot/claude-haiku-4.5",
                "copilot/claude-sonnet-4",
                "copilot/claude-sonnet-4.5",
                "copilot/gpt-4.1",
                "copilot/gpt-4o",
                "copilot/gpt-5",
                "copilot/o3-mini",
                "copilot/o4-mini"
            ]
        }
    },
    "cSpell.words": [
        "dataframe",
        "dcount",
        "groupby",
        "isort",
        "Jaquel",
        "Matplotlib",
        "notnull",
        "nullable",
        "odsbox",
        "pyplot",
        "rowlimit",
        "rowskip",
        "seaborn",
        "seqlimit",
        "seqskip",
        "stddev",
        "stdev",
        "submatrix",
        "venv"
    ],
    "[python]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.rulers": [
            119
        ]
    },
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": "explicit"
    },
    "python.testing.pytestArgs": [
        "tests"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true
}